{
  "extends": "tslint-config-airbnb",
  "rules": {
    "object-shorthand-properties-first": false,
    "no-increment-decrement": false,
    "import-name": false,
    "trailing-comma": false,
    "no-else-after-return": false,
    "no-boolean-literal-compare": false,
    "prefer-template": [
      false
    ],
    "quotemark": [
      false
    ],
    "max-line-length": [
      true,
      140
    ],
    "import-blacklist": [
      true,
      ".",
      ".."
    ]
  }
}